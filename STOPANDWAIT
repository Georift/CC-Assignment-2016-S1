compile			= "stopandwait.c"
winopen = true
maxmessagesize  = 256bytes

bandwidth		= 56Kbps,

messagerate             = 1000ms,
propagationdelay        = 2500ms,

probframecorrupt = 0

host perth {
    winx=0, winy=250
    
    outputfile="perth"
    link to sydney {
    }
}

host sydney {
    winx=550, winy=250
    outputfile="sydney"
    east east east east east of perth
}

host melbourne {
    winx=1100, winy=250
    outputfile="melbourne"
    south south of perth
    link to perth {
    }
}
